{
  "name": "docusaurus-theme-redoc",
  "version": "0.1.0",
  "description": "Redoc Component for DocusaurusV2",
  "main": "dist/index.js",
  "scripts": {
    "_copy": "npx copyup -e \"./**/*.js\" -e \"./**/*.ts\" \"./src/**/*.*\"",
    "build": "tsc -p tsconfig.jsx.json && tsc",
    "postbuild": "npm run _copy -- ./dist && npm run _copy -- ./dist-jsx",
