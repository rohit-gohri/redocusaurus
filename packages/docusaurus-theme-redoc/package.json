  },
  "homepage": "https://github.com/rohit-gohri/redocusaurus#readme",
  "dependencies": {
    "@docusaurus/types": "2.0.0-alpha.70",
    "clsx": "^1.1.1",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.0",
    "lodash": "^4.17.19",
    "mobx": "^6.0.4",
    "redoc": "^2.0.0-rc.48",
    "styled-components": "^5.2.1"
