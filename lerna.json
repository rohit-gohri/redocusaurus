{
  "npmClient": "yarn",
  "packages": ["website", "packages/*"],
  "command": {
    "publish": {
      "allowBranch": ["main"],
      "distTag": "latest",
      "ignoreChanges": ["*.md", "**/*.test.ts", "**/*.test.js"],
      "message": "chore(release): publish %s"
    },
    "version": {
      "message": "chore(release): %s"
    }
  },
  "version": "0.3.4"
}
